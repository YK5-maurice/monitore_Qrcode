<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Monitore_Qrcode</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Monitore_Qrcode.styles.css" asp-append-version="true" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.datatables.net/2.0.7/css/dataTables.bootstrap5.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/3.0.2/css/buttons.bootstrap5.css" asp-append-version="true" />
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Monitore_Qrcode<a  asp-controller="Home" asp-action="Data">Privacy</a>
       </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="https://cdn.datatables.net/2.0.7/js/dataTables.js" asp-append-version="true"></script>
    <script src="https://cdn.datatables.net/2.0.7/js/dataTables.bootstrap5.js" asp-append-version="true"></script>

    <script src="https://cdn.datatables.net/buttons/3.0.2/js/dataTables.buttons.js" asp-append-version="true"></script>
    <script src="https://cdn.datatables.net/buttons/3.0.2/js/buttons.bootstrap5.js" asp-append-version="true"></script>
    <script src="https://cdn.datatables.net/buttons/3.0.2/js/buttons.colVis.min.js" asp-append-version="true"></script>
    <script>
        $('#example').DataTable({
            layout: {
                topStart: {
                    buttons: ['colvis']
                }
            },
            scrollX: true
        });
    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
